{
    "name": "devzihad/env-editor",
    "description": "This package allows editing Laravel .env files programmatically.",
    "license": "MIT",
    "type": "library",
    "autoload": {
        "psr-4": {
            "Devzihad\\EnvEditor\\": "src/"
        }
    },
    "authors": [
        {
            "name": "Zihadul Islam",
            "email": "devzihad2022@gmail.com"
        }
    ],
    "require": {
        "php": ">=8.1"
    },
    "minimum-stability": "stable",
    "extra": {
        "laravel": {
            "providers": [
                "Devzihad\\EnvEditor\\EnvEditorServiceProvider"
            ]
        },
        "config": {
            "optimize-autoloader": true
        }
    }
}